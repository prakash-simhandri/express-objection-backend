# Database configuration
DB_NAME=xxxx          # Name of your MySql or postgres database
DB_USERNAME=xxxx                  # Username for connecting to your database
DB_PASSWORD=xxxx              # Password for the database user (leave blank if not needed)

# Authentication key used for various secured operations, e.g., API keys
AUTH_KEY=P@gmail.com      # Replace with your authorization key if required

# Secret key for token generation (e.g., JWT)
TOKEN_SECRET=@xxx        # Replace with your secret key for signing tokens
